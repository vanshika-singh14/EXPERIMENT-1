#include <iostream>
using namespace std;

int main() 
{

  int year;
  cout << "Enter a year: ";
  cin >> year;

  if (year % 400 == 0) {
    cout << year << " is a leap year.";
  }
 
  else if (year % 100 == 0) {
    cout << year << " is not a leap year.";
  }
  
  else if (year % 4 == 0) {
    cout << year << " is a leap year.";
  }

  else {
    cout << year << " is not a leap year.";
  }

  return 0;
}


/*
Output
Enter a year: 2019
2019 is not a leap year.
*/
